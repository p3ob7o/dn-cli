{
    "name": "automattic/dn-cli",
    "description": "CLI tool for Automattic Domain Services API",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": ">=8.0",
        "automattic/domain-services-client": "^1.6",
        "symfony/console": "^6.0 || ^7.0",
        "guzzlehttp/guzzle": "^7.0",
        "guzzlehttp/psr7": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "DnCli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DnCli\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "bin": ["bin/dn"],
    "minimum-stability": "stable"
}
